start:
    pnpm run start

lint:
    pnpm run lint

tc:
    pnpm run typecheck

unit:
    pnpm run unit-test

e2e:
    pnpm run package && pnpm run e2e-test

test:
    just unit && just e2e

codegen:
    npx tsx scripts/playwright-codegen.ts

print-py-nodes file:
    npx tsx scripts/print-py-nodes.ts {{file}}
